<?xml version="1.0" encoding="utf-8"?>

<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_home">

    <fragment
        android:id="@+id/navigation_home"
        android:name="com.example.antenatalcareapp.DocMainView"
        android:label="home"
        tools:layout="@layout/fragment_doc_main_view" />

    <fragment
        android:id="@+id/navigation_favourites"
        android:name="com.example.antenatalcareapp.Appointments"
        android:label="favourites"
        tools:layout="@layout/fragment_appointments" />

    <fragment
        android:id="@+id/navigation_search"
        android:name="com.example.antenatalcareapp.MotherChat"
        android:label="search"
        tools:layout="@layout/fragment_mother_chat" />

    <fragment
        android:id="@+id/navigation_profile"
        android:name="com.example.antenatalcareapp.ProfileFragment"
        android:label="profile"
        tools:layout="@layout/fragment_profile" />

</navigation>